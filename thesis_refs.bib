@article{WaterAbs,
author = {George M. Hale and Marvin R. Querry},
journal = {Appl. Opt.},
keywords = {},
number = {3},
pages = {555--563},
publisher = {OSA},
title = {Optical Constants of Water in the 200-nm to 200-$\mu$m Wavelength
Region},
volume = {12},
month = {Mar},
year = {1973},
url = {http://ao.osa.org/abstract.cfm?URI=ao-12-3-555},
doi = {10.1364/AO.12.000555},
abstract = {Extinction coefficients k($\lambda$) for water at 25{\textdegree}C
were determined through a broad spectral region by manually smoothing a point by
point graph of k($\lambda$) vs wavelength $\lambda$ that was plotted for data
obtained from a review of the scientific literature on the optical constants of
water. Absorption bands representing k($\lambda$) were postulated where data
were not available in the vacuum uv and soft x-ray regions. A subtractive
Kramers-Kronig analysis of the combined postulated and smoothed portions of the
k($\lambda$) spectrum provided the index of refraction n($\lambda$) for the
spectral region 200 nm $\leq$ $\lambda$ $\leq$ 200 $\mu$m.},
}

